{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "/scripts/bundler.sh"
      ],
      "matchStrings": [
        "JRE_RELEASE=[\"'](?<currentValue>.+?)[\"']\\s+"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "adoptium/temurin8-binaries"
    }
  ]
}
